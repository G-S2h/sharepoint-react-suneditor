{
  "name": "sharepoint-react-suneditor",
  "version": "1.0.5",
  "description": "A simple SunEditor React component wrapper made to work with SharePoint 2016 On-Premises",
  "keywords": ["sharepoint", "suneditor", "spfx", "wysiwyg"],
  "author": "Salah",
  "license": "MIT",
  "repository": "G-S2h/sharepoint-react-suneditor",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "del /q dist && webpack",
    "start": "webpack --watch --display none",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "15.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "15.4.2",
    "react-dom": "^15.4.2",
    "react-scripts": "^3.4.1",
    "webpack": "4.42.0",
    "webpack-cli": "^3.3.10"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "suneditor": "^2.30.0"
  }
}
